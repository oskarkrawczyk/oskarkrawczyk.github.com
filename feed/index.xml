---
layout: nil
---
<rss version="2.0">
  <channel>
    <title>Oskar Krawczyk - Nouincolor</title>
    <link>http://nouncolor.com/</link>
    <description></description>

    {% for post in site.posts %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <link>
          http://nouncolor.com{{ post.url | replace: '.html', ''  }}
        </link>
        <guid isPermaLink="false">
          http://nouncolor.com{{ post.url | replace: '.html', ''  }}
        </guid>
        <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
        <description>
          {{ post.content | xml_escape }}
        </description>
      </item>
    {% endfor %}
  </channel>
</rss>
